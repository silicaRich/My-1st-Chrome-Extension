{
    "name": "FB Focus",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Set & stick to your FB intentions",
    "homepage_url": "http://nicoleflokos.com",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "webNavigation",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "browser action demo",
        "default_popup": "src/browser_action/browser_action.html"
    }  
}